// tsconfig.paths.json

{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
    "@/hooks/*": ["./src/hooks/*"],
    "@/styles/*": ["./src/styles/*"],      "@/*": ["src/*"],
      "@app/*": ["src/app/*"],
      "@components/*": ["src/components/*"],
      "@contexts/*": ["src/contexts/*"],
      "@features/*": ["src/features/*"],
      "@hooks/*": ["src/hooks/*"],
      "@pages/*": ["src/pages/*"],
      "@services/*": ["src/services/*"],
      "@shared/*": ["src/shared/*"],
      "@store/*": ["src/store/*"],
      "@styles/*": ["src/styles/*"],
      "@types/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"],
      "@ui/*": ["src/ui/*"]
    }
  }
}
